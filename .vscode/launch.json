  "version": "0.2.0",
  "configurations": [
    {
      "name": "complete-integration",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/bootstarp",
      "windows": {
        "env": {
          "GOARCH": "amd64",
          "GOOS": "windows"
        }
      },
      "osx": {
        "env": {
          "GOARCH": "arm64",
          "GOOS": "darwin"
        }
      }
    },
    {
      "name": "generate-specific-modules",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/bootstarp",
      "args": [
          "-modules",
          "banner,cache,i18n"
      ],
      "windows": {
          "env": {
              "GOARCH": "amd64",
              "GOOS": "windows"
          }
      },
      "osx": {
          "env": {
              "GOARCH": "arm64",
              "GOOS": "darwin"
          }
      }
    }
  ]
}